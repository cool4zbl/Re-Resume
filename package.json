{
  "name": "re-resume",
  "version": "1.0.0",
  "description": "Another front end engineer resume generator",
  "private": true,
  "scripts": {
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "start": "npm run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "dev": "npm run env:dev -- webpack-dev-server",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build-ori": "npm run env:prod -- webpack",
    "build-resume": "PUBLIC_PATH=resume npm run build && cp -r dist/ ../blog/resume/",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "bundle": "webpack"
  },
  "author": {
    "name": "cool4zbl",
    "email": "i@zhangbinliu.me"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-actions": "^3.0.0",
    "@storybook/react": "^3.0.0",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.1.4",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^0.11.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "opn-cli": "^3.1.0",
    "prettier": "^1.5.3",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.7",
    "url-loader": "^0.5.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^4.1.1",
    "react-typography": "^0.15.10",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.1.2",
    "typography": "^0.15.12",
    "typography-plugin-code": "^0.15.11",
    "typography-theme-funston": "^0.15.10",
    "typography-theme-github": "^0.15.10",
    "typography-theme-stow-lake": "^0.15.10"
  },
  "keywords": [
    "resume",
    "online resume",
    "F2E resume",
    "front end engineer resume"
  ]
}
